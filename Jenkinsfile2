pipeline {
    agent any
    tools {
        maven 'M2_HOME'
    }
    stages {
        stage ('sonarqube scan'){
            steps{
                sh 'mvn verify org.sonarsource.scanner.maven:sonar-maven-plugin:sonar -Dsonar.projectKey=Theoalban_week16'
            }
        }
        stage ('all maven commands'){
            steps{
                sh 'mvn clean test compile install package'
            }
        }
        
    }
}